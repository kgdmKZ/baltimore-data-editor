<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="baltimore_neighborhoods" dynamic-insert="false" dynamic-update="false" mutable="true" name="Address" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="address">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <many-to-one class="CouncilDistrict" embed-xml="true" fetch="select" insert="true" name="councilDistrict" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="councildistrict" not-null="true"/>
        </many-to-one>
        <many-to-one class="Neighborhood" embed-xml="true" fetch="select" insert="true" name="neighborhood" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="neighborhood_id" not-null="true"/>
        </many-to-one>
        <many-to-one class="PoliceDistrict" embed-xml="true" fetch="select" insert="true" name="policeDistrict" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="policedistrict" not-null="true"/>
        </many-to-one>
        <property generated="never" lazy="false" name="streetAddress" optimistic-lock="true" type="string" unique="false">
            <column name="street_address"/>
        </property>
        <set embed-xml="true" fetch="select" inverse="true" lazy="true" mutable="true" name="libraries" optimistic-lock="true" sort="unsorted" table="library">
            <key on-delete="noaction">
                <column name="address_id" not-null="true" unique="true"/>
            </key>
            <one-to-many class="Library" embed-xml="true" not-found="exception"/>
        </set>
        <set embed-xml="true" fetch="select" inverse="true" lazy="true" mutable="true" name="groceryStores" optimistic-lock="true" sort="unsorted" table="grocery_store">
            <key on-delete="noaction">
                <column name="address_id" not-null="true" unique="true"/>
            </key>
            <one-to-many class="GroceryStore" embed-xml="true" not-found="exception"/>
        </set>
        <set embed-xml="true" fetch="select" inverse="true" lazy="true" mutable="true" name="locations" optimistic-lock="true" sort="unsorted" table="location">
            <key on-delete="noaction">
                <column name="address_id" not-null="true" unique="true"/>
            </key>
            <one-to-many class="Location" embed-xml="true" not-found="exception"/>
        </set>
    </class>
</hibernate-mapping>
