<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="baltimore_neighborhoods" dynamic-insert="false" dynamic-update="false" mutable="true" name="VacantBuilding" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="vacant_building">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <many-to-one class="Location" embed-xml="true" fetch="select" insert="true" name="location" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="location_id" not-null="true"/>
        </many-to-one>
        <property generated="never" lazy="false" name="referenceid" optimistic-lock="true" type="string" unique="false">
            <column name="referenceid" not-null="true" unique="true"/>
        </property>
        <property generated="never" lazy="false" name="noticedate" optimistic-lock="true" type="date" unique="false">
            <column length="10" name="noticedate" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
