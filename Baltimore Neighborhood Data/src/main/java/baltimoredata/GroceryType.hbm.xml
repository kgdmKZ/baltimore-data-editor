<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="baltimore_neighborhoods" dynamic-insert="false" dynamic-update="false" mutable="true" name="GroceryType" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="grocery_type">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property generated="never" lazy="false" name="name" optimistic-lock="true" type="string" unique="false">
            <column name="name" not-null="true" unique="true"/>
        </property>
        <set embed-xml="true" fetch="select" inverse="true" lazy="true" mutable="true" name="groceryStores" optimistic-lock="true" sort="unsorted" table="grocery_store">
            <key on-delete="noaction">
                <column name="type" not-null="true"/>
            </key>
            <one-to-many class="GroceryStore" embed-xml="true" not-found="exception"/>
        </set>
    </class>
</hibernate-mapping>
