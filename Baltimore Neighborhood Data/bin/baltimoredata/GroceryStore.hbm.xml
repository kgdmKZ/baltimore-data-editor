<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="baltimore_neighborhoods" dynamic-insert="false" dynamic-update="false" mutable="true" name="GroceryStore" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="grocery_store">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <many-to-one class="Address" embed-xml="true" fetch="select" insert="true" name="address" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="address_id" not-null="true" unique="true"/>
        </many-to-one>
        <many-to-one class="GroceryType" embed-xml="true" fetch="select" insert="true" name="groceryType" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="type" not-null="true"/>
        </many-to-one>
        <many-to-one class="ZipCode" embed-xml="true" fetch="select" insert="true" name="zipCode" not-found="exception" optimistic-lock="true" unique="false" update="true">
            <column name="zipcode" not-null="true"/>
        </many-to-one>
        <property generated="never" lazy="false" name="name" optimistic-lock="true" type="string" unique="false">
            <column name="name" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
